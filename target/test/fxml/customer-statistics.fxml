<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane alignment="CENTER" hgap="10.0" vgap="10.0" xmlns:fx="http://javafx.com/fxml"
          fx:controller="com.zf.product.doufu.controller.PrintController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <Label style="-fx-font: NORMAL 20 Tahoma;" text="日下单统计" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
    <Separator GridPane.columnIndex="0" GridPane.rowIndex="1" prefWidth="-Infinity"/>
    <HBox GridPane.columnIndex="0" GridPane.rowIndex="2">
        <ChoiceBox fx:id="monthChoice"/>
        <ChoiceBox fx:id="dayChoice"/>
        <Label prefWidth="400"/>
    </HBox>
    <Separator GridPane.columnIndex="0" GridPane.rowIndex="3"/>
    <VBox fx:id="tableViewBox" GridPane.columnIndex="0" GridPane.rowIndex="4"/>
</GridPane>
